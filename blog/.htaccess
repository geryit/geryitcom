RewriteCond %{REQUEST_FILENAME} -f
RewriteCond %{REQUEST_FILENAME} \.(gif|jpe?g?|png)$ [NC]
RewriteCond %{HTTP_REFERER} !^http(s)?://geryit\.com/blog [NC]
RewriteRule \.(gif|jpe?g?|png)$ - [F,NC,L]
